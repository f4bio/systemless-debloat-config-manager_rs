{
  "name": "systemless-debloat-config-manager",
  "version": "1.0.0",
  "description": "SystemlessDebloat Config Manager",
  "private": "true",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "export NODE_OPTIONS=--openssl-legacy-provider && npx webpack --config ./webpack.dev.js",
    "build:prod": "export NODE_OPTIONS=--openssl-legacy-provider && npx webpack --config ./webpack.prod.js",
    "clean": "npm run clean:dev",
    "clean:dev": "npx rimraf ./dist/ && npx rimraf ./target/ && npx rimraf ./pkg/",
    "serve": "npm run serve:dev",
    "serve:dev": "export NODE_OPTIONS=--openssl-legacy-provider && npx webpack serve --config ./webpack.dev.js",
    "lint": "npm run lint:dev",
    "lint:dev": "npx eslint ./www/",
    "lint:fix": "npx eslint --fix ./www/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f4bio/systemless-debloat-config-manager_rs.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack5",
    "bootstrap5",
    "scss"
  ],
  "author": "Fabio Tea <iam@f4b.io>",
  "bugs": {
    "url": "https://github.com/f4bio/systemless-debloat-config-manager_rs/issues"
  },
  "homepage": "https://github.com/f4bio/systemless-debloat-config-manager_rs#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@fontsource/fira-mono": "^4.5.8",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "babel-loader": "^8.2.5",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-css": "^0.6.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.1",
    "posthtml-load-config": "^2.0.0",
    "posthtml-loader": "^2.0.1",
    "prettier": "^2.6.2",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-config-dump-plugin": "^3.0.1",
    "webpack-dev-server": "^4.9.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.5.3"
  }
}
