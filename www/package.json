{
  "name": "systemless-debloat-config-manager",
  "version": "0.0.1",
  "description": "create an app to consume rust-generated wasm packages",
  "main": "index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "build:dev": "NODE_OPTIONS=--openssl-legacy-provider npx webpack --config webpack.dev.js",
    "build:prod": "NODE_OPTIONS=--openssl-legacy-provider npx webpack --config webpack.prod.js",
    "build": "npm run build:prod",
    "serve:dev": "NODE_OPTIONS=--openssl-legacy-provider npx webpack serve --config webpack.dev.js",
    "serve": "npm run serve:dev",
    "lint": "npm run lint:styles && npm run lint:html && npm run lint:js",
    "lint:js": "npx eslint --ext .js",
    "lint:html": "npx eslint --ext .html",
    "lint:styles": "npx stylelint **/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitlab.com/f4b.io/systemless-debloat-config-manager_rs.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": "Fabio Tea <iam@f4b.io>",
  "bugs": {
    "url": "https://gitlab.com/f4b.io/systemless-debloat-config-manager_rs/issues"
  },
  "homepage": "https://gitlab.com/f4b.io/systemless-debloat-config-manager_rs#readme",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "systemless-debloat-config-manager": "file:../pkg"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "autoprefixer": "^10.4.7",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-php-markup": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "highlight.js": "^11.5.1",
    "html-webpack-plugin": "^5.5.0",
    "img-optimize-loader": "^1.0.7",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.6.0",
    "sass": "^1.51.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-scss": "^4.2.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
}
