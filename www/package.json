{
  "name": "systemless-debloat-config-manager",
  "version": "0.0.1",
  "description": "create an app to consume rust-generated wasm packages",
  "main": "index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "build:dev": "NODE_OPTIONS=--openssl-legacy-provider npx webpack --config webpack.dev.js",
    "build:prod": "NODE_OPTIONS=--openssl-legacy-provider npx webpack --config webpack.prod.js",
    "build": "npm run build:prod",
    "serve:dev": "NODE_OPTIONS=--openssl-legacy-provider npx webpack serve --config webpack.dev.js",
    "serve": "npm run serve:dev",
    "lint": "npm run lint:styles && npm run lint:html && npm run lint:js",
    "lint:js": "npx eslint --ext .js",
    "lint:html": "npx eslint --ext .html",
    "lint:styles": "npx stylelint **/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f4bio/systemless-debloat-config-manager_rs.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": "Fabio Tea <iam@f4b.io>",
  "bugs": {
    "url": "https://github.com/f4bio/systemless-debloat-config-manager_rs/issues"
  },
  "homepage": "https://github.com/f4bio/systemless-debloat-config-manager_rs#readme",
  "dependencies": {
    "systemless-debloat-config-manager": "file:../pkg"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "autoprefixer": "^10.4.7",
    "bootstrap": "^5.2.0-beta1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^6.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.0",
    "posthtml": "^0.16.6",
    "posthtml-loader": "^2.0.1",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "svgo": "^2.8.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1",
    "webpack-merge": "^5.8.0",
    "webpack-pwa-manifest": "^4.3.0"
  }
}
